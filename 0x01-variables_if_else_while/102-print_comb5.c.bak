/**
* main - Entry point of program
*
* Return: 0 upon successful execution
*/
int main(void)
{
	int i, j, k, l, hwl, hwr, cwl, cwr, new_j, new_i;

	//printf("Start here\n\n");
	i = 48;
	while (i <= 57)
	{
		new_i = 1;
		j = 48;
		while (j <= 57)
		{
			new_j = 1;
			k = new_i == 1 && new_j == 1 ? i : 48;
			while (k <= 57)
			{
				l = new_j == 1 ? j : 48;
				hwl = i + j;
				hwr = k + l;
				while (l <= 57)
				{
					cwl = i + j;
					cwr = k + l;
					//printf("\nCur weight: [%d, %d] - heavy weight: [%d, %d]\n", cwl, cwr, hwl, hwr);
					int cnd1 = (i == j && j == k && k == l);
					int cnd2 = (hwl > cwl && hwr > cwr);
					//printf("\nTest 1: %d - Test 2: %d\n", cnd1, cnd2);
					if ((i == j && j == k && k == l) || (i == k && j == l))
					{
						l++;
						continue;
					}
					hwl = cwl;
					hwr = cwr;
					putchar(i);
					putchar(j);
					putchar(' ');
					putchar(k);
					putchar(l);
					if (i != 57 || j != 56 || k != 57 || l != 57)
					{
						putchar(',');
						putchar(' ');
					}
					l++;
				}
				k++;
				new_j = 0;
			}
			j++;
			new_i = 0;
		}
		i++;
	}
	putchar('\n');
	return (0);
}
